							<g:Hyperlink text="Modifier le mot de passe" targetHistoryToken="password" ui:field="cmdPasswordProf"/>
							<g:Hyperlink targetHistoryToken="!contact" text="Aide / Contact" ui:field="cmdHelpProf"/>
						</g:TreeItem>						
