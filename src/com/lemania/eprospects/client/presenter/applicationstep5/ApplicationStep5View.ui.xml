<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:SimplePanel ui:field="main">
    	<g:VerticalPanel width="100%" height="" horizontalAlignment="ALIGN_CENTER">
    		<g:Label text="MEDICAL ET REGIME ALIMENTAIRE" styleName="pageTitle"/>
    		<g:Label text="Step 5 of 5"/>
    		<g:VerticalPanel width="900px" styleName="subSection" horizontalAlignment="ALIGN_CENTER">
    			<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
    				<g:VerticalPanel horizontalAlignment="ALIGN_LEFT" width="80%" spacing="10">
    				<g:Label text="ELEMENTS CHOISIS" styleName="sectionTitle"/>
    				<g:FlexTable width="100%"/>
    				<g:HorizontalPanel width="100%">
    					<g:Label text="Total" styleName="lablel_TotalAmount"/>
    					<g:Label text="Amount" styleName="lablel_TotalAmount"/>
    				</g:HorizontalPanel>
    			</g:VerticalPanel>
    			</g:VerticalPanel>
    				<g:Label text="." styleName="hiddenText"/>
    				<g:CheckBox text="J&apos;ai lu, compris et signé les conditions financières et générale, la politique d&apos;annulaltion et/ou les politiques de départ anticipé." width=""/>
    				<g:Grid width="80%">
    					<g:row>
    						<g:customCell>
    							<g:Label text="Lieu / Date" styleName="fieldTitle"/>
    						</g:customCell>
    						<g:customCell>
    							<g:TextBox styleName="sis_textbox"/>
    						</g:customCell>
    						<g:customCell>
    							<g:Label text="Nom / Prénom" styleName="fieldTitle"/>
    						</g:customCell>
    						<g:customCell>
    							<g:TextBox styleName="sis_textbox"/>
    						</g:customCell>
    					</g:row>
    				</g:Grid>
    			</g:VerticalPanel>
    		<g:FlowPanel width="900px">
    			<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
    				<g:HorizontalPanel width="20%" horizontalAlignment="ALIGN_RIGHT" spacing="10">
    				<g:Button ui:field="cmdPreviousStep">Précédant</g:Button>
    			<g:Button ui:field="cmdNextStep" text="Suivant">Next Step</g:Button>
    		</g:HorizontalPanel>
    			</g:VerticalPanel>
    		</g:FlowPanel>
    	</g:VerticalPanel>
    </g:SimplePanel>
</ui:UiBinder>
